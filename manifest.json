{
  "manifest_version": 2,
  "name": "CSVHub",
  "description": "Shows useful diffs for CSV files on GitHub",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "coopyhx.js",
        "script.js"
      ],
      "css": [
        "coopyhx.css"
      ],
      "run_at": "document_end"
    }
  ]
}
