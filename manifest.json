{
  "manifest_version": 2,
  "name": "CSVHub",
  "description": "Shows useful diffs for CSV files on GitHub",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/coopyhx.js",
        "js/jquery-1.11.1.js",
        "js/jquery.csv-0.71.js",
        "csvhub.js"
      ],
      "css": [
        "css/coopyhx.css"
      ],
      "run_at": "document_end"
    }
  ]
}
