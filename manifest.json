{
  "manifest_version": 2,
  "name": "CSVHub",
  "description": "Shows useful diffs for CSV files on GitHub",
  "version": "1.0.2",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/daff-1.1.8.js",
        "js/jquery-1.11.1.js",
        "js/jquery.csv-0.71.js",
        "csvhub.js"
      ],
      "css": [
        "css/daff.css"
      ],
      "run_at": "document_end"
    }
  ]
}
